{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "website",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start"],
      "autoAttachChildProcesses": true,
      "stopOnEntry": true,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "rollup build:esm-bundle",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "build"],
      "autoAttachChildProcesses": true,
      "stopOnEntry": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
